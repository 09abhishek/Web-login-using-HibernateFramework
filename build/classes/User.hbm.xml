<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="hiberproj.UserEntity" table="USER">
      	  <id name="uid" column="UID" />
         <property name="uname"/>
        <property name="upass"/>
